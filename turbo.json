{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["@repo/ui#generate", "^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "typecheck": {
      "dependsOn": ["@repo/ui#generate", "^typecheck"]
    },
    "dev": {
      "dependsOn": ["@repo/ui#generate"],
      "persistent": true,
      "cache": false
    },
    "analyze:ci": {
      "dependsOn": ["build"],
      "outputs": [".next/analyze/**"]
    },
    "clean": {
      "cache": false
    },
    "generate": {}
  }
}
